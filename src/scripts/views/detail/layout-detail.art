{{each list}}
    <header class="detail-header-scroll">
        <div class="header-back">
            <img src="data:image/png;base64,">
        </div>
        <span class="header-title">{{$value.bigbook_name}}</span>
        <div class="header-index">
            <img src="data:image/png;base64,">
        </div>
        <div class="header-share">
            <img src="data:image/png;base64,">
        </div>
    </header>
    <header class="detail-header">
        <div class="header-cover"></div>
        <div class="header-group">
            <div class="header-box">
                <div class="header-pic"
                    style="background-image: url({{$value.coverurl}});">
                </div>
            </div>
            <div class="header-info">
                <p class="info-title">{{$value.bigbook_name}}</p>
                <p class="info-type">{{$value.key_name}}</p>
                <div class="info-star">
                    <div class="star-red"><img src="data:image/png;base64,"></div>
                    <div class="star-red"><img src="data:image/png;base64,"></div>
                    <div class="star-red"><img src="data:image/png;base64,"></div>
                    <div class="star-red"><img src="data:image/png;base64,"></div>
                    <div class="star-white"><img src="data:image/png;base64,"></div>
                </div>
                <p class="info-fans">人气：{{$value.collectcount}}</p>
            </div>
        </div>
    </header>
    <section class="detail-tab">
        <div data-to="produce" class="tab-item tab-border">简介</div>
        <div data-to="catalog" class="tab-item tab-border active">目录</div>
        <div data-to="comment" class="tab-item">评论</div>
    </section>
    <div class="content"></div>
{{/each}}